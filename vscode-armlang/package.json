{
  "name": "vscode-armlang",
  "displayName": "vscode-armlang",
  "description": "",
  "version": "0.0.1",
  "scripts": {
    "convert": "ts-node src/convert"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "armlang",
        "aliases": [
          "ArmLang",
          "armlang"
        ],
        "extensions": [
          ".arm"
        ]
      }
    ],
    "grammars": [
      {
        "language": "armlang",
        "scopeName": "source.armlang",
        "path": "./armlang.grammar.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "yamljs": "^0.3.0"
  }
}